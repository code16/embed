{
    "name": "code16/embed",
    "type": "library",
    "description": "Embed laravel blade component + validation",
    "keywords": [
        "code16",
        "embed",
        "laravel",
        "package"
    ],
    "homepage": "https://github.com/code16/embed",
    "license": "MIT",
    "author": "Code 16",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Code16\\Embed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Embed\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Code16\\Embed\\EmbedServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
